{
  "tencent_cos": {
    "region": "",
    "bucket": "",
    "dotenv_path": "./.env"
  },
  "polling": {
    "start_hhmm": "11:20",
    "end_hhmm": "12:00",
    "interval_seconds": 20
  },
  "local": {
    "download_dir": "./temp_cos_downloads",
    "state_file": "./state/cos_daily_state.json"
  },
  "targets": {
    "dayahead_node_price": {
      "prefix": "4、组织中心/5、日前节点电价查询/data/day_ahead",
      "date_offsets_days_priority": [
        0
      ]
    },
    "realtime_node_price": {
      "prefix": "4、组织中心/6、实时节点电价查询/data/real_time",
      "min_size_bytes": 100000,
      "date_offsets_days_priority": [
        -2
      ]
    },
    "info_disclose_forecast": {
      "prefix": "3、披露中心/1、信息披露查询/data/info_disclose/预测",
      "date_offsets_days_priority": [
        1
      ]
    },
    "info_disclose_actual": {
      "prefix": "3、披露中心/1、信息披露查询/data/info_disclose/实际",
      "date_offsets_days_priority": [
        -1,
        -2
      ]
    }
  },
  "limits": {
    "max_download_import_per_day": 4,
    "max_per_type_per_day": 1
  }
}
